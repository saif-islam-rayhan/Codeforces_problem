#include<bits/stdc++.h>
using namespace std;
int main(){
    int test; cin >> test;
    while (test--) {
        int n; cin >> n;
        int a[n];
        for (int i = 0;i < n;i++) {
            cin >> a[i];
        }
        int mx = -1;
        for (int i = 0;i < n;i+=2) {
            mx = max(mx, a[i]);
        }
        cout << mx << endl;
    }
    return 0;
}
