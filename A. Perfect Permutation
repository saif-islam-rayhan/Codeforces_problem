#include<bits/stdc++.h>
using namespace std;
void solve(){
	int i,n;
	cin >> n;
	if(n == 1){
		cout << 1 <<endl;
		return;
	}
	for(i = 2;i <= n - 2;i += 2){
		cout << i << " " << i - 1 << " ";
	}
	if(n % 2 == 1){
		cout << n << " " << n - 2 << " " << n - 1 <<endl;
	}
	else cout << n << " " << n - 1 <<endl;
}
int main(){

    int t;
    cin >> t;
	while(t--){
		solve();
	}
}
        		  		 	  						 	   	
