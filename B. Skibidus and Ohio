#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        string s;
        cin>>s;
        int n=s.size();
        int temp=false;
        for(int i=0;i<n-1;i++)
        {
            if(s[i]==s[i+1])
            {
                temp=true;
                break;
            }

        }
        if(temp==false)
        {
            cout<<n<<endl;
        }
        else
        {
            cout<<"1"<<endl;
        }
    }
}
